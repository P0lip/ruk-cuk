{
  "name": "ruk-cuk",
  "version": "0.2.1",
  "description": "Generate Moleculer-compliant TypeScript typings from 3.x OpenAPI documents",
  "keywords": [],
  "homepage": "https://github.com/P0lip/ruk-cuk",
  "bugs": {
    "url": "https://github.com/P0lip/ruk-cuk/issues"
  },
  "packageManager": "npm@8",
  "engines": {
    "node": ">=14.17"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "type": "module",
  "bin": {
    "ruk-cuk": "./bin/index.mjs"
  },
  "exports": {
    ".": "./dist/codegen/index.mjs"
  },
  "imports": {
    "#cli/*": "./dist/cli/*.mjs",
    "#codegen": "./dist/codegen/index.mjs",
    "#config": "./dist/config/index.mjs"
  },
  "sideEffects": false,
  "license": "Apache-2.0",
  "author": "P0lip",
  "repository": {
    "type": "git",
    "url": "https://github.com/P0lip/ruk-cuk"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint": "ls-lint && eslint --cache --cache-location .cache/ --ext=.mjs src/",
    "test": "NODE_ENV=test c8 mocha --config .mocharc ./**/__tests__/**/*.test.mjs",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@ls-lint/ls-lint": "^1.10.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/yargs": "^17.0.7",
    "c8": "^7.8.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "mocha": "^8.0.1",
    "mocha-each": "^2.0.1",
    "prettier": "^2.5.1",
    "rollup": "^2.56.2"
  },
  "dependencies": {
    "@babel/generator": "^7.16.5",
    "@babel/parser": "^7.16.6",
    "@babel/types": "^7.16.0",
    "@stoplight/json": "^3.17.1",
    "ajv": "^8.6.3",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.2",
    "es-aggregate-error": "^1.0.7",
    "refless-json-schema-to-typescript": "0.0.0",
    "yargs": "^17.3.0"
  }
}
