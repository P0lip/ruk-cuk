{
  "openapi": "3.1.0",
  "info": {
    "title": "Pets"
  },
  "paths": {
    "/v1/pets": {
      "post": {
        "operationId": "v1.petstore.createPet",
        "responses": {
          "200": {
            "$ref": "#/paths/~1v1~1pets/get/responses/200"
          }
        }
      },
      "get": {
        "operationId": "v1.petstore.getPets",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer"
                      },
                      "kind": {
                        "enum": [
                          "Dog",
                          "Cat"
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
